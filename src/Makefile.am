bin_PROGRAMS = bitc
AM_CPPFLAGS = $(DEPS_CFLAGS)
bitc_LDADD = $(DEPS_LIBS) $(CURL_LIBS) $(LEVELDB_LIBS) $(SNAPPY_LIBS)
bitc_LDADD += -lpanel -lform -lstdc++

bitc_CFLAGS  = -O1 -MMD -g
bitc_CFLAGS += -Wextra -Wno-unused-parameter -Wno-sign-compare -Wno-missing-field-initializers
bitc_CFLAGS += -fno-omit-frame-pointer
bitc_CFLAGS += -fstack-protector

bitc_SOURCES  = btc-message.c
bitc_SOURCES += ncui.c
bitc_SOURCES += script.c
bitc_SOURCES += bitc_ui.c
bitc_SOURCES += peer.c
bitc_SOURCES += peergroup.c
