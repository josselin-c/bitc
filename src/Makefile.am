bitc_SOURCES += block-store.c
bitc_SOURCES += hash.c
bitc_SOURCES += fx.c
bitc_SOURCES += base58.c
bitc_SOURCES += bloom.c
bitc_SOURCES += key.c
bitc_SOURCES += txdb.c
bitc_SOURCES += wallet.c
bitc_SOURCES += test.c
bitc_SOURCES += serialize.c
bitc_SOURCES += hashtable.c
bitc_SOURCES += MurmurHash3.c
bitc_SOURCES += util.c
bitc_SOURCES += file.c
bitc_SOURCES += poolworker.c
bitc_SOURCES += config.c
bitc_SOURCES += poll.c
bitc_SOURCES += netasync.c
bitc_SOURCES += main.c
bitc_SOURCES += cJSON.c
bitc_SOURCES += ip_info.c
